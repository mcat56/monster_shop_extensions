<h1> <%= @address.nickname.capitalize %></h1>

<p>Street: <%= @address.address %></p>
<p>City: <%= @address.city %></p>
<p>State: <%= @address.state %></p>
<p>Zip: <%= @address.zip %></p>

<%= link_to 'Update Address', "/users/#{@address.user.id}/addresses/#{@address.id}/edit" %>
<%= link_to 'Delete Address', "/users/#{@address.user.id}/addresses/#{@address.id}", method: :delete %>
